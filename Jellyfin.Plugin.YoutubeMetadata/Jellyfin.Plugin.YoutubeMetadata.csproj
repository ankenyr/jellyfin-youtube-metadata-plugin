	  </Reference>
	  <Reference Include="MediaBrowser.Controller">
	    <HintPath>..\..\jellyfin\MediaBrowser.Controller\bin\Debug\net6.0\MediaBrowser.Controller.dll</HintPath>
	  </Reference>
	  <Reference Include="MediaBrowser.Model">
	    <HintPath>..\..\jellyfin\MediaBrowser.Controller\bin\Debug\net6.0\MediaBrowser.Model.dll</HintPath>
	  </Reference>
	  <Reference Include="NYoutubeDL">
